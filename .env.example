# Her Law Family Court Organizer - Environment Variables Template

# App Configuration
VITE_APP_NAME="Her Law Family Court Organizer"
VITE_APP_VERSION="0.1.0"
VITE_APP_ENVIRONMENT="production"

# Security Configuration
VITE_ENCRYPTION_KEY="your-encryption-key-here-generate-a-secure-32-byte-key"
VITE_SESSION_TIMEOUT=300000

# Storage Configuration
VITE_LOCAL_STORAGE_ENCRYPTED=true
VITE_MAX_FILE_SIZE=10485760

# Feature Flags
VITE_ENABLE_BIOMETRIC_AUTH=true
VITE_ENABLE_PRIVACY_SCREEN=true
VITE_ENABLE_PDF_EXPORT=true
VITE_ENABLE_OFFLINE_MODE=true

# Cloud Sync Configuration (Optional - for email/password cloud backup)
# Get these from: https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_URL=your-project-url-here
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration (if needed in future)
# VITE_API_URL=https://api.yourbackend.com
# VITE_API_TIMEOUT=30000
